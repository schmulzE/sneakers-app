{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-tsx-scope": "off"
    },
    "globals": {
      "React": "writable"
    },
    "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "browser": true,
    "amd": true,
    "node": true
  }
}